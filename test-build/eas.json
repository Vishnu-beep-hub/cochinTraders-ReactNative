{
  "cli": {
    "version": ">= 16.28.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "BASE_URL": "$BASE_URL",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "$EXPO_PUBLIC_GEOAPIFY_API_KEY",
        "API_KEY": "$API_KEY"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "BASE_URL": "$BASE_URL",
        "EXPO_PUBLIC_GEOAPIFY_API_KEY": "$EXPO_PUBLIC_GEOAPIFY_API_KEY",
        "API_KEY": "$API_KEY"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
